    width: 5px;
    margin: -3px 0;       /* 중앙 정렬 */
    border-radius: 2px;
}
QSlider::handle:horizontal:hover{
    background: rgb(100, 150, 250);  /* 원하는 눌림 색상 */
    border: none;
}
QSlider::handle:horizontal:pressed {
    background: rgb(100, 150, 250);  /* 원하는 눌림 색상 */
    border: none;
}

